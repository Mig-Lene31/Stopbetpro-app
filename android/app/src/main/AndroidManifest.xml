<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.stopbet.app">

    <uses-permission android:name="android.permission.FOREGROUND_SERVICE"/>
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW"/>
    <uses-permission android:name="android.permission.BIND_ACCESSIBILITY_SERVICE"/>

    <application
        android:allowBackup="true"
        android:label="KairÃ³s"
        android:theme="@android:style/Theme.DeviceDefault.NoActionBar">

        <activity android:name=".BootActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".InfoActivity"/>
        <activity android:name=".GateActivity"/>
        <activity android:name=".ConfigActivity"/>
        <activity android:name=".DepositActivity"/>
        <activity android:name=".LimitsActivity"/>
        <activity android:name=".TimeActivity"/>
        <activity android:name=".RulesActivity"/>
        <activity android:name=".BlockedActivity"/>
        <activity android:name=".AdminHiddenActivity"/>

        <service
            android:name=".EngineService"
            android:exported="false"/>

        <service
            android:name=".StopHeartService"
            android:exported="false"/>

        <service
            android:name=".BalanceReadAccessibilityService"
            android:permission="android.permission.BIND_ACCESSIBILITY_SERVICE"
            android:exported="false">
            <intent-filter>
                <action android:name="android.accessibilityservice.AccessibilityService"/>
            </intent-filter>
        </service>

    </application>
</manifest>
